IMGSENSOR_SUBDRV_OBJ := \
	$(subdrv-rpath)/ov13b10_ofilm_front_mipiraw_Sensor.o 

ifeq ($(CONFIG_MTK_IMGSENSOR_ISP3_Z), m)
$(eval $(MTK_PLATFORM_KO)-objs += $(IMGSENSOR_SUBDRV_OBJ))
else ifeq ($(CONFIG_MTK_IMGSENSOR_ISP3_Z), y)
obj-$(CONFIG_MTK_IMGSENSOR_ISP3_Z) += $(IMGSENSOR_SUBDRV_OBJ)
endif
ifeq ($(CONFIG_MTK_IMGSENSOR_ISP4), m)
$(eval $(MTK_PLATFORM_KO)-objs += $(IMGSENSOR_SUBDRV_OBJ))
else ifeq ($(CONFIG_MTK_IMGSENSOR_ISP4), y)
obj-$(CONFIG_MTK_IMGSENSOR_ISP4) += $(IMGSENSOR_SUBDRV_OBJ)
else ifeq ($(CONFIG_MTK_IMGSENSOR_ISP3_M), m)
$(eval $(MTK_PLATFORM_KO)-objs += $(IMGSENSOR_SUBDRV_OBJ))
else ifeq ($(CONFIG_MTK_IMGSENSOR_ISP3_M), y)
obj-$(CONFIG_MTK_IMGSENSOR_ISP3_M) += $(IMGSENSOR_SUBDRV_OBJ)
endif

ifeq ($(CONFIG_MTK_IMGSENSOR_ISP4_C), m)
$(eval $(MTK_PLATFORM_KO)-objs += $(IMGSENSOR_SUBDRV_OBJ))
else ifeq ($(CONFIG_MTK_IMGSENSOR_ISP4_C), y)
obj-$(CONFIG_MTK_IMGSENSOR_ISP4_C) += $(IMGSENSOR_SUBDRV_OBJ)
endif

